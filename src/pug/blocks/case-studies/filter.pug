.filter 
    .filter__header 
        button.filter__btn.active All case studies
        button.filter__btn Social Media
        button.filter__btn Seo
        button.filter__btn Research
        button.filter__btn Content & PR
        button.filter__btn Payed Traffic
    .filter__wrap
        .filter__item
            .item-filter
                .item-filter__picture
                    img.item-filter__img(src="" alt="")
                    .item-filter__info
                        img.item-filter__logo(src="" alt="")
                        .item-filter__label
                .item-filter__content
                    .ranges
                        .ranges__wrap
                            +ranges("50 100", "blue" , "300%", "first mixin")
                            +ranges("80 100", "red" , "100%", "second mixin")